{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Serilog": {
        "Using": [],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ],
        "WriteTo": []
    },
    "Jwt": {
        "SecretKey": "MyVeryLongSecretKeyThatShouldBeAtLeast256BitsLongForHS256Algorithm",
        "Issuer": "HomeBook",
        "Audience": "HomeBook",
        "ExpirationMinutes": 60
    },
    "Version": "1.0.96"
}
